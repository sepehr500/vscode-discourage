{
  "name": "discourage",
  "displayName": "Discourage",
  "description": "Visual Studio Code extension that adds little discouragement while you work",
  "version": "0.0.2",
  "publisher": "sepehr500",
  "repository": {
    "url": "https://github.com/sepehr500/vscode-discourage"
  },
  "engines": {
    "vscode": "^1.0.0"
  },
  "author": {
    "name": "Sepehr Sobhani"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "discourage.toggle",
        "title": "Toggle Discourage"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  }
}
